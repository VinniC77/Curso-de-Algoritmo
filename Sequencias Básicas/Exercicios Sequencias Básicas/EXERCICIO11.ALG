algoritmo "Exercicio11"
var
   A, B, C, D: Real
inicio
      Escreval ("Digite três números para descobrirmos o valor de Delta entre eles: ")
      Escreva ("Numero 1: ")
      Leia (A)
      Escreva ("Numero 2: ")
      Leia (B)
      Escreva ("Numero 3: ")
      Leia (C)
      D <- (B^2) - (4*A*C)
      Escreva ("O valor de Delta entre", A, ", ", B, " e ", C, " é igual a ", D)
fimalgoritmo