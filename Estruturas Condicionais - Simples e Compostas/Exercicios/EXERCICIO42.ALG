algoritmo "Exercicio42"
var
   contador, num: Inteiro
inicio
      Escreva ("Quer contar até quanto? ")
      Leia (num)
      contador <- 0
      Enquanto (contador < num) faca
               contador <- contador + 1
               Escreva (contador)
      FimEnquanto
                 Escreva (" Acabou!")
fimalgoritmo