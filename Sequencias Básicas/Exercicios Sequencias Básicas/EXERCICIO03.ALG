algoritmo "Exercicio03"
var
   nomeFuncionario: caractere
   salario: real
inicio
      salario <- 1850.45
      Escreval ("Digite o nome do funcionário: ", nomeFuncionario)
      Leia (nomeFuncionario)
      Escreva ("O funcionário(a) ", nomeFuncionario, " tem um salário de ",salario, " em Junho.")
fimalgoritmo