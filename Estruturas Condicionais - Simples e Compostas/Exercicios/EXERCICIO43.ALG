algoritmo "Exercicio43"
var
   contador: Inteiro
inicio
      contador <- 31
      Enquanto (contador >= 1) faca
      contador <- contador - 1
      Se contador = (contador % 4 = 0) entao
         Escreva ("[",contador,"]")
      senao
           Escreva (contador)
      FimSe
      FimEnquanto
fimalgoritmo