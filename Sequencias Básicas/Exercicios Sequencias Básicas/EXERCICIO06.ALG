algoritmo "Exercicio06"
var
   num, ant, suc: inteiro
inicio
      Escreval ("Digite um número: ")
      Leia (num)
      ant <- num - 1
      suc <- num + 1
      Escreval ("O antecessor do número ", num, " é o número ",ant)
      Escreva ("O sucessor do número ", num, " é o número ", suc)
fimalgoritmo