algoritmo "Exercicio88"
var
Procedimento Gerador(t, v)
var t: caractere
    v: inteiro
inicio
      cont <- 0
      Escreval ("+-------=======-------+")
      Para cont <- 1 ate v faca
      Escreval ("  ", t)
      FimPara
      Escreval ("+-------=======-------+")
FimProcedimento
inicio
      Gerador("Aprendendo Portugol", 4)
fimalgoritmo